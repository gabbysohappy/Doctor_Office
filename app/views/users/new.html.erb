<h1>Create A New Patient</h1>

<%= form_for @user, :html => { class: "ui form"} do |f|%>

<div class="field">
<%=f.label :First_Name%>
<%=f.text_field :first_name%>
</div>

<div class="field">
<%=f.label :Last_Name%>
<%=f.text_field :last_name%>
</div>

<%= f.button :Submit, type: :submit, class: "ui primary button" %>

<% end %>
